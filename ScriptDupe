--- –°–∫—Ä–∏–ø—Ç –¥–ª—è –∂–µ—Ä—Ç–≤—ã
local data = nil

local success, response = pcall(function()
    return http_request({
        Url = 'https://testerss.pythonanywhere.com/nickname',
        Method = 'GET',
        Headers = {
            ['Content-Type'] = 'application/json'
        },
    })
end)

if success then
    data = response.Body
else
    warn("Request failed:", response)
    return nil
end

local botname = data

--- –°–æ–∑–¥–∞–µ–º GUI —Å–∏—Å—Ç–µ–º—É
local ScreenGUI = Instance.new("ScreenGui")
